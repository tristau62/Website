<h1>New Circuit</h1>
<%= form_for :circuit, url: circuits_path do |f| %>
 


  <p>
    <%= f.label :carrier %><br>
    <%= f.text_field :carrier %>
  </p>
 
  <p>
    <%= f.label :contract %><br>
    <%= f.text_field :contract %>
  </p>
  <p>
    <%= f.label :purpose %><br>
    <%= f.text_area :purpose %>
  </p>
  <p>
    <%= f.label :expiration %><br>
    <%= f.text_field :expiration %>
  </p>
  <p>
    <%= f.label :rate %><br>
    <%= f.text_field :rate %>
  </p>
 
   <%= f.submit %>
  
  <p>
   
  </p>
  <%= link_to 'Circuits', circuits_path %>
<% end %>